<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".HomeActivity">
    <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
              android:id="@+id/tv_title"
              android:text="SOPTHub"
              android:textSize="24sp"
              app:layout_constraintStart_toStartOf="parent"
              app:layout_constraintEnd_toEndOf="parent"
              app:layout_constraintTop_toTopOf="parent"
              android:layout_margin="30dp"
    />
    <ImageView android:layout_width="100dp"
               android:layout_height="100dp"
               android:id="@+id/iv_image"
               app:layout_constraintDimensionRatio="1:1"
               app:layout_constraintTop_toBottomOf="@id/tv_title"
               android:layout_margin="30dp"
               app:layout_constraintStart_toStartOf="parent"
               app:layout_constraintEnd_toEndOf="parent"
               app:srcCompat="@drawable/dogi"/>
    <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
              android:id="@+id/tv_nameTitle"
              android:text="이름"

              app:layout_constraintStart_toStartOf="parent"
              app:layout_constraintEnd_toEndOf="parent"
              app:layout_constraintTop_toBottomOf="@id/iv_image"
              android:layout_marginTop="30dp"
              android:layout_marginRight="150dp"
    />
    <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
              android:id="@+id/tv_name"
              android:text="이종찬"
              app:layout_constraintEnd_toEndOf="parent"
              app:layout_constraintStart_toStartOf="parent"
              app:layout_constraintTop_toTopOf="@id/tv_nameTitle"
              android:layout_marginLeft="150dp"/>
    <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
              android:id="@+id/tv_ageTitle"
              android:text="나이"
              app:layout_constraintTop_toBottomOf="@+id/tv_nameTitle"
              app:layout_constraintStart_toStartOf="@id/tv_nameTitle"
              app:layout_constraintEnd_toEndOf="@id/tv_nameTitle"
              android:layout_marginTop="20dp"/>
    <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
              android:id="@+id/tv_age"
              android:text="25"
              app:layout_constraintTop_toBottomOf="@id/tv_name"
              app:layout_constraintStart_toStartOf="@id/tv_name"
              app:layout_constraintEnd_toEndOf="@id/tv_name"
              android:layout_marginTop="20dp"/>
    <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
              android:id="@+id/tv_MBTI_Title"
              android:text="MBTI"
              app:layout_constraintTop_toBottomOf="@+id/tv_ageTitle"
              app:layout_constraintStart_toStartOf="@id/tv_ageTitle"
              app:layout_constraintEnd_toEndOf="@id/tv_ageTitle"
              android:layout_marginTop="20dp"/>
    <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
              android:id="@+id/tv_MBTI"
              android:text="ESTJ"
              app:layout_constraintTop_toBottomOf="@id/tv_age"
              app:layout_constraintStart_toStartOf="@id/tv_age"
              app:layout_constraintEnd_toEndOf="@id/tv_age"
              android:layout_marginTop="20dp"/>
    <Button android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:id="@+id/bt_follower"
            android:text="팔로워\n목록"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/tv_MBTI"
            android:backgroundTint="#EC8DAD"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="60dp"
    />
    <Button android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:id="@+id/bt_repository"
            android:text="레포지토리\n목록"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/tv_MBTI"
            android:backgroundTint="#DDBD5E"
            android:layout_marginTop="10dp"
            android:layout_marginRight="60dp"
    />
    <androidx.fragment.app.FragmentContainerView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/container"
            android:padding="16dp"
            tools:layout = "@layout/fragment_repository"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/bt_follower"
            android:layout_margin="15dp"
    />
